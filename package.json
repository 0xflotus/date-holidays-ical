{
  "name": "date-holidays-ical",
  "version": "0.1.3-0",
  "description": "Generate ical files from date-holidays data",
  "keywords": [
    "holidays",
    "ical",
    "vcalendar",
    "world"
  ],
  "homepage": "https://github.com/commenthol/date-holidays-ical#readme",
  "bugs": {
    "url": "https://github.com/commenthol/date-holidays-ical/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/date-holidays-ical.git"
  },
  "license": "ISC",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "bin": {
    "holidays-ical": "bin/holidays-ical.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "build": "babel -d lib src",
    "clean": "rimraf lib coverage .nyc_output",
    "clean:all": "npm-run-all clean clean:node_modules",
    "clean:node_modules": "rimraf node_modules",
    "coverage": "nyc -r text -r html npm test",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "no-console": 2
    }
  },
  "dependencies": {
    "commander": "^2.14.1",
    "date-holidays": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
